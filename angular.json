{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "portal-client": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/portal-admi",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/styles.scss",
              "src/assets/css/color-1.css",
              "src/assets/css/vendors/font-awesome.css",
              "src/assets/css/vendors/icofont.css",
              "src/assets/css/vendors/themify.css",
              "src/assets/css/vendors/flag-icon.css",
              "src/assets/css/vendors/feather-icon.css",
              "src/assets/css/vendors/scrollbar.css",
              "src/assets/css/vendors/animate.css",
              "src/assets/css/vendors/date-picker.css",
              "src/assets/css/vendors/photoswipe.css",
              "src/assets/css/vendors/bootstrap.css",
              "src/assets/css/style.css",
              "src/assets/css/responsive.css"
            ],
            "scripts": [
              "src/assets/js/jquery-3.5.1.min.js",
              "src/assets/js/bootstrap/bootstrap.bundle.min.js",
              "src/assets/js/icons/feather-icon/feather.min.js",
              "src/assets/js/icons/feather-icon/feather-icon.js",
              "src/assets/js/scrollbar/simplebar.js",
              "src/assets/js/scrollbar/custom.js",
              "src/assets/js/config.js",
              "src/assets/js/sidebar-menu.js",
              "src/assets/js/chart/knob/knob.min.js",
              "src/assets/js/chart/knob/knob-chart.js",
              "src/assets/js/chart/apex-chart/apex-chart.js",
              "src/assets/js/chart/apex-chart/stock-prices.js",
              "src/assets/js/notify/bootstrap-notify.min.js",
              "src/assets/js/dashboard/default.js",
              "src/assets/js/notify/index.js",
              "src/assets/js/datepicker/date-picker/datepicker.js",
              "src/assets/js/datepicker/date-picker/datepicker.en.js",
              "src/assets/js/datepicker/date-picker/datepicker.custom.js",
              "src/assets/js/photoswipe/photoswipe.min.js",
              "src/assets/js/photoswipe/photoswipe-ui-default.min.js",
              "src/assets/js/photoswipe/photoswipe.js",
              "src/assets/js/typeahead/handlebars.js",
              "src/assets/js/typeahead/typeahead.bundle.js",
              "src/assets/js/typeahead/typeahead.custom.js",
              "src/assets/js/typeahead-search/handlebars.js",
              "src/assets/js/typeahead-search/typeahead-custom.js",
              "src/assets/js/height-equal.js",
              "src/assets/js/script.js",
              "src/assets/js/theme-customizer/customizer.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "7md",
                  "maximumError": "7mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5mb",
                  "maximumError": "5mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "local": {
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "portal-client:build:production"
            },
            "local": {
              "browserTarget": "portal-client:build:local"
            },
            "development": {
              "browserTarget": "portal-client:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "portal-client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/styles.scss",
              "src/assets/css/vendors/font-awesome.css",
              "src/assets/css/vendors/icofont.css",
              "src/assets/css/vendors/themify.css",
              "src/assets/css/vendors/flag-icon.css",
              "src/assets/css/vendors/feather-icon.css",
              "src/assets/css/vendors/scrollbar.css",
              "src/assets/css/vendors/scrollable.css",
              "src/assets/css/vendors/animate.css",
              "src/assets/css/vendors/date-picker.css",
              "src/assets/css/vendors/photoswipe.css",
              "src/assets/css/vendors/bootstrap.css",
              "src/assets/css/style.css",
              "src/assets/css/responsive.css",
              "assets/css/color-1.css"
            ],
            "scripts": [
              "src/assets/js/jquery-3.5.1.min.js",
              "src/assets/js/bootstrap/bootstrap.bundle.min.js",
              "src/assets/js/icons/feather-icon/feather.min.js",
              "src/assets/js/icons/feather-icon/feather-icon.js",
              "src/assets/js/scrollbar/simplebar.js",
              "src/assets/js/scrollbar/custom.js",
              "src/assets/js/config.js",
              "src/assets/js/sidebar-menu.js",
              "src/assets/js/script.js",
              "src/assets/js/theme-customizer/customizer.js"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "portal-client"
}
